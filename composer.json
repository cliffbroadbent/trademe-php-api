{
    "name": "goldfinch/trademe-php-api",
    "description": "A PHP client for the Trade Me API",
    "type": "library",
    "license": "MIT",
    "keywords": ["trademe", "trade-me"],
    "authors": [
        {
            "name": "JP Caparas",
            "email": "jp@jpcaparas.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "JPCaparas\\TradeMeAPI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.8",
        "guzzlehttp/oauth-subscriber": "^0.8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "phpspec/prophecy": "^1.18",
        "symfony/var-dumper": "^6.3",
        "php-coveralls/php-coveralls": "^2.7",
        "phpspec/prophecy-phpunit": "^2.4"
    }
}
